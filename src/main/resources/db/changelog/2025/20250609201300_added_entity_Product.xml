<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <changeSet id="20250609201300-1" author="stdmk">
        <createTable tableName="product" schemaName="bot">
            <column name="id" autoIncrement="true" type="bigint">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="userid" type="bigint">
                <constraints nullable="true" />
            </column>
            <column name="name" type="text">
                <constraints nullable="true" />
            </column>
            <column name="caloric" type="decimal">
                <constraints nullable="true" />
            </column>
            <column name="proteins" type="decimal">
                <constraints nullable="true" />
            </column>
            <column name="fats" type="decimal">
                <constraints nullable="true" />
            </column>
            <column name="carbs" type="decimal">
                <constraints nullable="true" />
            </column>
        </createTable>
    </changeSet>

</databaseChangeLog>
